<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.pocketfresh.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.pocketfresh.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.pocketfresh.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <activity
        android:id="@+id/home_screen2"
        android:name="com.example.pocketfresh.home_screen"
        android:label="activity_launch_screen"
        tools:layout="@layout/activity_launch_screen" />
    <activity
        android:id="@+id/liked_items2"
        android:name="com.example.pocketfresh.liked_items"
        android:label="activity_liked_items"
        tools:layout="@layout/activity_liked_items" />
    <activity
        android:id="@+id/launch_screen2"
        android:name="com.example.pocketfresh.launch_screen"
        android:label="activity_search_screen"
        tools:layout="@layout/activity_search_screen" />
</navigation>