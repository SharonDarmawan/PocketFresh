<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".launch_screen"
    android:background="#FFFBED">

    <ImageView
        android:id="@+id/account"
        android:layout_width="45dp"
        android:layout_height="51.8dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="45dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048"
        app:srcCompat="@drawable/account" />

    <TextView
        android:id="@+id/hi_bob"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Hi, Bob!"
        android:textColor="#193A1A"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />

    <TextView
        android:id="@+id/search_produce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Search produce"
        android:textColor="#193A1A"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.074" />





    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="360dp"
        android:layout_height="70dp"
        android:layout_marginTop="140dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.94"
        app:srcCompat="@drawable/search" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="148dp"
        android:ems="10"
        android:hint="Try typing 'banana'"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.383"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.207" />

    <LinearLayout
        android:id="@+id/search_results"
        android:layout_width="348dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:layout_constraintVertical_bias="0.186">


        <Button
            android:id="@+id/item2"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rounded_corner_item_button"
            android:drawableLeft="@drawable/apple"
            android:drawableRight="@drawable/front_arrow"
            android:fontFamily="@font/poppins_regular"
            android:gravity="center_vertical"
            android:text="Apple\nMay - June"
            android:textAllCaps="false"
            android:textSize="18sp"
            app:backgroundTint="@null"
            tools:layout_editor_absoluteX="24dp" />

        <Button
            android:id="@+id/item3"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="350dp"
            android:layout_height="71dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rounded_corner_item_button"
            android:drawableLeft="@drawable/peach"
            android:drawableRight="@drawable/front_arrow"
            android:fontFamily="@font/poppins_regular"
            android:gravity="center_vertical"
            android:text="Peach\nMay - June"
            android:textAllCaps="false"
            android:textSize="18sp"
            app:backgroundTint="@null"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="400dp" />

        <Button
            android:id="@+id/item4"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="350dp"
            android:layout_height="71dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rounded_corner_item_button"
            android:drawableLeft="@drawable/orange"
            android:drawableRight="@drawable/front_arrow"
            android:fontFamily="@font/poppins_regular"
            android:gravity="center_vertical"
            android:text="Orange\nMay - August"
            android:textAllCaps="false"
            android:textSize="18sp"
            app:backgroundTint="@null"
            tools:layout_editor_absoluteX="24dp" />

        <Button
            android:id="@+id/item5"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="350dp"
            android:layout_height="71dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rounded_corner_item_button"
            android:drawableLeft="@drawable/blackberry"
            android:drawableRight="@drawable/front_arrow"
            android:fontFamily="@font/poppins_regular"
            android:gravity="center_vertical"
            android:text="Blackberry\nAll year round"
            android:textAllCaps="false"
            android:textSize="18sp"
            app:backgroundTint="@null"
            tools:layout_editor_absoluteX="25dp" />






    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
